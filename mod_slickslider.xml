                <field name="slide_i" type="media" label="Slide I" />
                <field name="slide_j" type="media" label="Slide J" />
                <field name="slide_k" type="media" label="Slide K" />
                <field name="slide_l" type="media" label="Slide L" />
                <field name="slide_m" type="media" label="Slide M" />
                <field name="slide_n" type="media" label="Slide N" />
                <field name="slide_o" type="media" label="Slide O" />
                <field name="slide_p" type="media" label="Slide P" />
                <field name="slide_q" type="media" label="Slide Q" />
                <field name="slide_u" type="media" label="Slide U" />
                <field name="slide_r" type="media" label="Slide R" />
                <field name="slide_s" type="media" label="Slide S" />
                <field name="slide_t" type="media" label="Slide T" />
                <field name="slide_u" type="media" label="Slide U" />
                <field name="slide_v" type="media" label="Slide V" />
                <field name="slide_w" type="media" label="Slide W" />
                <field name="slide_x" type="media" label="Slide X" />
                <field name="slide_y" type="media" label="Slide Y" />
                <field name="slide_z" type="media" label="Slide Z" />
            </fieldset>
            <fieldset name="links" label="Links">
                <field name="link_a" type="url" label="Link A" />
                <field name="link_b" type="url" label="Link B" />
                <field name="link_c" type="url" label="Link C" />
                <field name="link_d" type="url" label="Link D" />
                <field name="link_e" type="url" label="Link E" />
                <field name="link_f" type="url" label="Link F" />
                <field name="link_g" type="url" label="Link G" />
