<?xml version="1.0" encoding="utf-8"?>
<extension type="module" version="3.1.0" client="site" method="upgrade">
    <name>Slick Slider!</name>
    <author>codeChap</author>
    <version>1.0.0</version>
    <description>A simple Slick Slider module.</description>
    <files>
        <folder>css</folder>
        <folder>css/fonts</folder>
        <folder>js</folder>
        <filename>css/slick.css</filename>
        <filename>css/slick-theme.css</filename>
        <filename>css/fonts/slick.eot</filename>
        <filename>css/fonts/slick.svg</filename>
        <filename>css/fonts/slick.ttf</filename>
        <filename>css/fonts/slick.woff</filename>
        <filename>js/slick.min.js</filename>
        <filename>mod_slickslider.xml</filename>
        <filename module="mod_slickslider">mod_slickslider.php</filename>
        <filename>index.html</filename>
        <filename>tmpl/default.php</filename>
        <filename>tmpl/index.html</filename>
    </files>
    <config>
        <fields name="params">
            <fieldset name="images" label="Images">
                <field name="slide_a" type="media" label="Slide A" />
                <field name="slide_b" type="media" label="Slide B" />
                <field name="slide_c" type="media" label="Slide C" />
