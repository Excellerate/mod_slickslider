                <field name="link_y" type="url" label="Link Y" />
                <field name="link_z" type="url" label="Link Z" />
            </fieldset>
            <fieldset name="basic">
                <field name="height" type="text" label="Height" description="Height of the slider in pixels" default="384" />
                <field name="cover" type="checkbox" label="Brand Element" description="Overlay with brand element" value="1" default="0" />
                <field name="autoplay" type="checkbox" label="Autoplay" description="Start slider automatically" value="1" default="0" />
                <field name="css_ease" type="checkbox" label="Easing" description="Ease effect" value="1" default="0" />
                <field name="show_arrows" type="checkbox" label="Show arrows" description="Start next and prev arrows" value="1" default="0" />
                <field name="jumble" type="checkbox" label="Jumble images" description="Mix up the images" value="1" default="0" />
                <field name="show_dots" type="checkbox" label="Show dots" description="Show dots in the slider" value="1" default="0" />
                <field name="slides_to_show" type="textbox" label="Slides to show" description="How many slides per view" value="1" default="1" />
            </fieldset>
        </fields>
    </config>
    <updateservers>
        <server type="extension" name="Slickslider" priority="1">http://www.github.com/codechap/mod_slickslider/update.xml</server>
    </updateservers>
    <scriptfile>script.php</scriptfile>
</extension>